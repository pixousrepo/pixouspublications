---
import { PortableText } from "astro-portabletext";
import HeadingSubPage from "../blocks/HeadingSubPage.astro";
import NormalText from "../blocks/NormalText.astro";
import ulist3 from "../blocks/ulist3.astro";
import ListItemLevel3 from "../blocks/ListItemLevel3.astro";
const { data, components } = Astro.props;
const defaultComponents = {
  block: {
    h1: HeadingSubPage,
    normal: NormalText,
  },
  list: ulist3,
  listItem: ListItemLevel3,
};
---

<PortableText
  value={data.richText}
  components={{ ...defaultComponents, ...components }}
/>
